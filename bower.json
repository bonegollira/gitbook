{
  "name": "gitbook",
  "version": "0.0.0",
  "description": "note book based on Github",
  "dependencies": {
    "vue": "0.11.4",
    "marked": "0.3.2",
    "github-markdown-css": "2.0.1",
    "normalize.css": "3.0.2",
    "highlight.js": "git@github.com:isagalaev/highlight.js.git#8.4",
    "superagent": "git@github.com:visionmedia/superagent.git#0.21.0"
  },
  "overrides": {
    "vue": {
      "main": "dist/vue.min.js"
    },
    "marked": {
      "main": "lib/marked.js"
    },
    "gitub-markdown-css": {
      "main": "github-markdown.css"
    },
    "normalize.css": {
      "main": "normalize.css"
    },
    "highlight.js": {
      "main": [
        "build/highlight.pack.js",
        "build/demo/styles/github.css"
      ]
    },
    "superagent": {
      "main": "superagent.js"
    }
  }
}
